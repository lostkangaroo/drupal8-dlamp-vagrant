{
    "apache": {
        "version": "2.4",
        "package": "httpd",
        "devel_package": "httpd-devel",
        "binary": "/usr/sbin/httpd",
        "mpm": "prefork",
        "dir": "/etc/httpd",
        "lib_dir": "/usr/lib64/httpd",
        "libexec_dir": "/usr/lib64/httpd/modules",
        "cache_dir": "/var/cache/httpd",
        "cgibin_dir": "/var/www/cgi-bin",
        "docroot_dir": "/var/www/html",
        "conf": "/etc/httpd/conf/httpd.conf",
        "perl_pkg": "perl",
        "log_dir": "/var/log/httpd",
        "root_group": "root",
        "service_name": "httpd",
        "service_reload_command": "/sbin/service httpd graceful",
        "default_site_name": "default",
        "default_site_enabled": false,
        "default_modules": [
            "status",
            "alias",
            "auth_basic",
            "authn_file",
            "authz_core",
            "authz_groupfile",
            "authz_host",
            "authz_user",
            "autoindex",
            "dir",
            "env",
            "mime",
            "negotiation",
            "setenvif",
            "systemd"
        ],
        "mod_ssl": {
            "pkg_name": "mod_ssl"
        },
        "mod_php5": {
            "so_filename": "libphp5.so"
        }
    }
}
