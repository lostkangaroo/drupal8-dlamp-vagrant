{
    "apache": {
        "version": "2.4",
        "package": "apache2",
        "devel_package": "httpd-devel",
        "binary": "/usr/sbin/httpd2",
        "mpm": "prefork",
        "apachectl": "/usr/sbin/apache2ctl",
        "dir": "/etc/apache2",
        "lib_dir": "/usr/lib64/apache2",
        "libexec_dir": "/usr/lib64/apache2",
        "cache_dir": "/var/cache/apache2",
        "cgibin_dir": "/srv/www/cgi-bin",
        "docroot_dir": "/srv/www/htdocs",
        "conf": "/etc/apache2/httpd.conf",
        "perl_pkg": "perl",
        "log_dir": "/var/log/apache2",
        "root_group": "root",
        "service_name": "apache2",
        "default_site_name": "default",
        "default_site_enabled": false,
        "default_modules": [
            "status",
            "alias",
            "auth_basic",
            "authn_file",
            "authz_core",
            "authz_groupfile",
            "authz_host",
            "authz_user",
            "autoindex",
            "dir",
            "env",
            "mime",
            "negotiation",
            "setenvif"
        ],
        "mod_auth_openid": {
            "version": "0.8"
        },
        "mod_ssl": {
            "pkg_name": "mod_ssl"
        },
        "mod_php5": {
            "so_filename": "libphp5.so"
        }
    }
}
